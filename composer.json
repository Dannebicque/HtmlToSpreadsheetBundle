{
  "name": "davidannebicque/html-to-spreadsheet-bundle",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "bundle",
    "html",
    "excel",
    "spreadsheet",
    "phpspreadsheet"
  ],
  "description": "A Symfony bundle to convert HTML tables to Excel spreadsheets using PhpSpreadsheet.",
  "require": {
    "phpoffice/phpspreadsheet": "^4.0 || ^5.0",
    "symfony/dependency-injection": "^6.4 || ^7.0 || ^8.0",
    "symfony/framework-bundle": "^6.4 || ^7.0 || ^8.0",
    "symfony/config": "^6.4 || ^7.0 || ^8.0",
    "symfony/http-foundation": "^6.4 || ^7.0 || ^8.0",
    "symfony/console": "^6.4 || ^7.0 || ^8.0",
    "symfony/twig-bundle": "^6.4 || ^7.0 || ^8.0",
    "ext-dom": "*",
    "ext-libxml": "*"
  },
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "symfony/phpunit-bridge": "^6.4 || ^7.0 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Davidannebicque\\HtmlToSpreadsheetBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Davidannebicque\\HtmlToSpreadsheetBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "davidannebicque",
      "email": "david.annebicque@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
